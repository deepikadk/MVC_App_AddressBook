@model IEnumerable<MVCContactBook.ViewModel.ContactModel>
@{
    ViewBag.Title = "Contact List";
    var grid = new WebGrid(source: Model, canPage: true, canSort: true, rowsPerPage: 10);
}

<h3>Organization and People</h3>
<div style="overflow:auto">
        @*To list the Organization and People at home page.*@
        @grid.GetHtml(
        tableStyle: "table table-responsive",
        columns:grid.Columns
            (
                grid.Column("FirstName", "FirstName"),
                grid.Column("LastName", "LastName"),
                grid.Column("OrganizationName", "OrganizationName")
                
            )
        ) 
</div>